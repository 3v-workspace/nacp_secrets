  "paths": {
    "/v2/documents/{id}": {
      "get": {
        "summary": "NACP Retrieve Declaration",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "UUID"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "title": "Щорічна декларація V3",
                      "type": "object",
                      "properties": {
                        "id": {
                          "title": "Id",
                          "type": "string",
                          "format": "uuid"
                        },
                        "corruption_affected": {
                          "$ref": "#/definitions/CorruptionAffected"
                        },
                        "data": {
                          "$ref": "#/definitions/DataV3"
                        },
                        "declaration_type": {
                          "title": "Declaration Type",
                          "enum": [
                            1
                          ],
                          "type": "integer"
                        },
                        "declaration_year": {
                          "title": "Рік декларації",
                          "minimum": 1800,
                          "maximum": 2021,
                          "type": "integer"
                        },
                        "post_category": {
                          "$ref": "#/definitions/models__declaration__PostCategory"
                        },
                        "post_type": {
                          "$ref": "#/definitions/PostType"
                        },
                        "responsible_position": {
                          "title": "Responsible Position",
                          "type": "integer"
                        },
                        "schema_version": {
                          "title": "Schema Version",
                          "enum": [
                            3
                          ],
                          "type": "integer"
                        },
                        "type": {
                          "$ref": "#/definitions/Type"
                        },
                        "user_declarant_id": {
                          "title": "User Declarant Id",
                          "type": "integer"
                        },
                        "date": {
                          "title": "Date",
                          "type": "string",
                          "format": "date-time"
                        }
                      },
                      "required": [
                        "id",
                        "corruption_affected",
                        "data",
                        "declaration_type",
                        "declaration_year",
                        "post_category",
                        "post_type",
                        "responsible_position",
                        "schema_version",
                        "type",
                        "user_declarant_id",
                        "date"
                      ],
                      "additionalProperties": false
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/v2/documents": {
      "get": {
        "summary": "NACP List Declarations",
        "parameters": [
          {
            "in": "query",
            "name": "user_declaration_id",
            "schema": {
              "type": "number"
            }
          },
          {
            "in": "query",
            "name": "declaration_type",
            "schema": {
              "type": "number",
              "minimum": 0,
              "maximum": 5
            }
          },
          {
            "in": "query",
            "name": "declaration_year",
            "schema": {
              "type": "number",
              "maximum": 2021,
              "minimum": 2015
            }
          },
          {
            "in": "query",
            "name": "page",
            "schema": {
              "type": "number",
              "minimum": 1,
              "maximum": 100
            }
          }
        ]
      }
    }
  }
